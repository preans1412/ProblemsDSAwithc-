

// first apporach is using the sort function which is already included in c++


class Solution {
public:
    char findTheDifference(string s, string t) 
    {
       //string s; 
       //getline (cin, s);   
    sort(s.begin(),s.end());
    
    sort(t.begin(),t.end());
    
    
    for(int i=0;i<s.length();i++)
    {
      if(s[i]!=t[i])
      {
        return t[i];
      }
    }
    
    return t[t.length()-1];
      
    }
};
