
#include <iostream>

using namespace std;

int main()
{ 
    int n,r;
    
    cin>>n;
    int temp=n;
    int sum=0;
    
    while(n>0)
    {
         r= n%10;
        sum= (sum*10)+r;
        n=n/10;
    }
  if(temp==sum)
  {
      cout<<"palindrome ";
  }
  else
  {
      cout<<"not";
      
  }
  return 0;
    
}



